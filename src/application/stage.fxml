<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.StackPane?>

<?import javafx.scene.layout.HBox?>
<HBox fx:controller="application.Controller" xmlns:fx="http://javafx.com/fxml" >
    <ImageView fx:id="left">
        <Image url="/img/mix/mix-1.jpg"/>
    </ImageView>
    <StackPane>
        <ImageView fx:id="middle"><Image url="/img/eat/eat-1.jpg"/></ImageView>
        <Label fx:id="coordsLabel" text="Starting..." prefWidth="300" prefHeight="400" textFill="#ffffff">
            <font>
                <Font size="40"/>
            </font>
        </Label>
    </StackPane>
    <ImageView fx:id="right">
        <Image url="/img/go/go-1.jpg"/>
    </ImageView>
</HBox>